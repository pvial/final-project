    </th>

    <th>
    </th>
  </tr>

  <% @ejercicios.each do |ejercicio| %>
  <tr>
    <td>
      <%= ejercicio.id %>
    </td>

    <td>
      <%= ejercicio.aprobado %>
    </td>

    <td>
      <%= ejercicio.contenido_id %>
